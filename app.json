{
  "expo": {
    "name": "Portfolio Buddy",
    "description": "Track stocks, news, and portfolio changes. No accounts.",
    "slug": "portfolio",
    "privacy": "public",
    "version": "2.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "githubUrl": "https://github.com/t73liu/portfolio",
    "sdkVersion": "33.0.0",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#CDC449"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "io.github.t73liu.portfolio",
      "supportsTablet": true
    },
    "android": {
      "package": "io.github.t73liu.portfolio",
      "permissions": [],
      "versionCode": 7
    }
  }
}
