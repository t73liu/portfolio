{
  "name": "portfolio",
  "version": "1.1.0",
  "private": true,
  "devDependencies": {
    "@types/expo": "^27.0.8",
    "@types/is-number": "^3.0.1",
    "@types/jest": "^23.3.1",
    "@types/react": "^16.3.18",
    "@types/react-native": "0.55.28",
    "@types/react-native-svg-charts": "^5.0.1",
    "@types/react-navigation": "^2.0.19",
    "@types/react-redux": "^6.0.6",
    "@types/react-test-renderer": "^16.0.2",
    "@types/uuid": "^3.4.4",
    "jest-expo": "^29.0.0",
    "npm-run-all": "^4.1.3",
    "prettier": "1.14.2",
    "react-native-scripts-ts": "1.15.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.3.1",
    "ts-jest": "^23.1.4",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "~2.9.2"
  },
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "test": "jest",
    "format": "npm-run-all format:*",
    "format:json": "prettier --write {.,**}/*.json",
    "format:md": "prettier --write {.,**}/*.md",
    "format:ts": "prettier --write {.,**}/*.{ts,tsx} && tslint --fix -p ."
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "apisauce": "^0.16.0",
    "expo": "^29.0.0",
    "format-number": "^3.0.0",
    "is-number": "^7.0.0",
    "native-base": "~2.6.1",
    "rambda": "^1.2.2",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-29.0.0.tar.gz",
    "react-native-svg-charts": "^5.2.0",
    "react-navigation": "^2.12.1",
    "react-redux": "^5.0.7",
    "redux": "4.0.0",
    "redux-observable": "1.0.0",
    "redux-persist": "^5.10.0",
    "redux-persist-expo-filesystem": "^1.0.1",
    "rxjs": "6.3.1",
    "typesafe-actions": "^2.0.4",
    "uuid": "^3.3.2"
  },
  "resolutions": {
    "@types/react-native": "0.55.28",
    "@types/react": "16.3.18",
    "react": "16.3.1",
    "redux": "4.0.0",
    "rxjs": "6.3.1"
  }
}
